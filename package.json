{
  "name": "mm",
  "version": "0.0.0",
  "description": "parse markdown",
  "license": "MIT",
  "keywords": [],
  "repository": "micromark/mm",
  "bugs": "https://github.com/micromark/mm/issues",
  "funding": [
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/unifiedjs"
    },
    {
      "type": "OpenCollective",
      "url": "https://opencollective.com/unified"
    }
  ],
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "John Otander <johnotander@gmail.com> (https://johno.com)",
    "Stephan Schneider <stephanschndr@gmail.com>",
    "Victor Felder <victor@draft.li> (https://draft.li)",
    "Mudit Ameta <zeusdeux@gmail.com> (https://mudit.xyz)",
    "Christian Murphy <christian.murphy.42@gmail.com>",
    "Marouane Fazouane <fazouanem3@gmail.com>",
    "Merlijn Vos <merlijn@soverin.net>"
  ],
  "files": [
    "lib/",
    "buffer.js",
    "stream.js",
    "index.js"
  ],
  "dependencies": {
    "debug": "^4.0.0",
    "parse-entities": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-unassert": "^3.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.0.0",
    "commonmark.json": "^0.29.0",
    "concat-stream": "^2.0.0",
    "ms": "^2.0.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "regenerate": "^1.0.0",
    "remark-cli": "^8.0.0",
    "remark-preset-wooorm": "^7.0.0",
    "resolve-from": "^5.0.0",
    "tape": "^5.0.0",
    "tinyify": "^3.0.0",
    "unicode-13.0.0": "^0.8.0",
    "xo": "^0.33.0"
  },
  "scripts": {
    "generate-expressions": "node script/generate-expressions",
    "generate": "npm run generate-expressions",
    "format": "remark . -qfo && prettier --write . && xo --fix",
    "build-buffer": "browserify lib/buffer -t [ babelify --plugins [ babel-plugin-unassert ./script/babel-transform-undebug.js ./script/babel-transform-constants.js ] ] -p tinyify -s micromark -o micromark-buffer.min.js; gzip-size micromark-buffer.min.js; gzip-size --raw micromark-buffer.min.js",
    "build-stream": "browserify lib/stream -t [ babelify --plugins [ babel-plugin-unassert ./script/babel-transform-undebug.js ./script/babel-transform-constants.js ] ] -p tinyify -s micromark -o micromark-stream.min.js; gzip-size micromark-stream.min.js; gzip-size --raw micromark-stream.min.js",
    "build": "npm run build-buffer && npm run build-stream",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run generate && npm run format && npm run build && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "complexity": "off",
      "import/no-unassigned-import": "off",
      "no-lonely-if": "off",
      "no-unmodified-loop-condition": "off",
      "unicorn/explicit-length-check": "off",
      "unicorn/prefer-includes": "off",
      "unicorn/prefer-number-properties": "off",
      "unicorn/prefer-type-error": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
