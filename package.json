{
  "name": "micromark",
  "version": "2.11.4",
  "description": "small commonmark compliant markdown parser with positional info and concrete tokens",
  "license": "MIT",
  "keywords": [
    "commonmark",
    "compiler",
    "gfm",
    "html",
    "lexer",
    "markdown",
    "markup",
    "md",
    "unified",
    "parse",
    "parser",
    "plugin",
    "process",
    "remark",
    "render",
    "renderer",
    "token",
    "tokenizer"
  ],
  "repository": "micromark/micromark",
  "bugs": "https://github.com/micromark/micromark/issues",
  "funding": [
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/unifiedjs"
    },
    {
      "type": "OpenCollective",
      "url": "https://opencollective.com/unified"
    }
  ],
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Bogdan Chadkin <trysound@yandex.ru>",
    "Christian Murphy <christian.murphy.42@gmail.com>",
    "Marouane Fazouane <fazouanem3@gmail.com>",
    "John Otander <johnotander@gmail.com> (https://johno.com)",
    "Stephan Schneider <stephanschndr@gmail.com>",
    "Victor Felder <victor@draft.li> (https://draft.li)",
    "Mudit Ameta <zeusdeux@gmail.com> (https://mudit.xyz)",
    "Merlijn Vos <merlijn@soverin.net>"
  ],
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dist/",
    "lib/",
    "index.js",
    "index.d.ts",
    "stream.d.ts",
    "stream.js"
  ],
  "dependencies": {
    "@types/debug": "^4.0.0",
    "debug": "^4.0.0",
    "parse-entities": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/concat-stream": "^1.6.0",
    "@types/events": "^3.0.0",
    "@types/regenerate": "^1.0.0",
    "@types/tape": "^4.0.0",
    "@unicode/unicode-13.0.0": "^1.0.0",
    "babel-plugin-inline-constants": "^3.0.0",
    "babel-plugin-unassert": "^3.0.0",
    "babel-plugin-undebug": "^2.0.0",
    "c8": "^7.0.0",
    "character-entities": "^2.0.0",
    "commonmark.json": "^0.30.0",
    "concat-stream": "^2.0.0",
    "cross-env": "^7.0.0",
    "eslint-plugin-es": "^4.0.0",
    "eslint-plugin-security": "^1.0.0",
    "glob": "^7.0.0",
    "gzip-size-cli": "^5.0.0",
    "jsfuzz": "1.0.14",
    "ms": "^2.0.0",
    "patch-package": "^6.0.0",
    "prettier": "^2.0.0",
    "regenerate": "^1.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "tape": "^5.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "xo": "^0.39.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build-lib": "rimraf \"dist/\" \"{lib,test}/**/*.d.ts\" && node script/generate-expressions.js && tsc && type-coverage",
    "build-dist": "cross-env BUILD=dist rollup -c --silent && node script/copy-types.js",
    "build-size": "cross-env BUILD=size rollup -c --silent && gzip-size micromark.min.js && gzip-size --raw micromark.min.js",
    "build": "npm run build-lib && npm run build-dist && npm run build-size",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "pretest-fuzz": "patch-package --patch-dir script/patches && node script/generate-fixtures.js",
    "test-fuzz": "timeout 15m jsfuzz test/fuzz.cjs test/fixtures",
    "test-api": "node test/index.js",
    "test-coverage": "c8 --check-coverage --lines 100 --functions 100 --branches 100 --reporter lcov node test/index.js",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "envs": [
      "shared-node-browser"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "always"
      ],
      "prefer-arrow-callback": "off"
    },
    "overrides": [
      {
        "files": [
          "lib/**/*.js"
        ],
        "plugin": [
          "es"
        ],
        "extends": [
          "plugin:es/no-new-in-es2016",
          "plugin:security/recommended"
        ],
        "rules": {
          "complexity": "off",
          "guard-for-in": "off",
          "max-depth": "off",
          "security/detect-object-injection": "off",
          "unicorn/no-this-assignment": "off"
        }
      },
      {
        "files": [
          "**/*.d.ts"
        ],
        "rules": {
          "import/extensions": [
            "error",
            "never"
          ]
        }
      },
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "import/no-unassigned-import": "off"
        }
      },
      {
        "files": [
          "script/**/*.js"
        ],
        "rules": {
          "no-await-in-loop": "off"
        }
      }
    ],
    "ignores": [
      "dist/",
      "**/*.d.ts",
      "micromark.test.ts"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "lint-no-html",
        false
      ]
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreCatch": true,
    "ignoreFiles": [
      "lib/constant/assign.d.ts"
    ]
  }
}
